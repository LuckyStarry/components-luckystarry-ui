{
  "name": "luckystarry-ui",
  "version": "0.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "del-cli dist -f",
    "lint": "tslint -p . -c tslint.json 'src/luckystarry/**/*.ts'",
    "prebuild": "npm run clean && npm run lint",
    "build": "tsc",
    "postbuild": "npm run bundle",
    "bundle": "browserify dist/index.js > dist/index.bundle.js",
    "postbundle": "npm run uglify",
    "uglify": "uglifyjs dist/index.bundle.js --compress --mangle --output dist/index.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:LuckyStarry/components-luckystarry-ui.git"
  },
  "author": "SUN BO",
  "dependencies": {
    "axios": "^0.18.0",
    "element-ui": "^2.4.8",
    "qs": "^6.5.2",
    "uuid": "^3.3.2",
    "vue": "^2.5.17",
    "vue-class-component": "^6.3.2",
    "vue-property-decorator": "^7.2.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "del-cli": "^1.1.0",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.1.3",
    "uglify-js": "^3.4.9"
  },
  "bugs": {
    "url": "https://github.com/LuckyStarry/components-luckystarry-ui/issues"
  },
  "homepage": "https://github.com/LuckyStarry/components-luckystarry-ui",
  "description": "",
  "typings": "types/index.d.ts"
}
